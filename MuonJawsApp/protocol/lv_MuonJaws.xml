      <param name="Message" type="string">
        <read method="GCV" target="Message"/>
      </param>
      <param name="Position" type="float64">
        <read method="GCV" target="Position"/>
      </param>
      <param name="Set_Point" type="float64">
        <read method="GCV" target="Set Point"/>
        <set method="SCV" extint="true" target="Set Point" post_button="Set" post_button_wait="true"/>
      </param>
      <param name="Voltage_to_go_to" type="float64">
        <read method="GCV" target="Voltage to go to"/>
      </param>
      <param name="Present_Voltage" type="float64">
        <read method="GCV" target="Present Voltage"/>
      </param>
